{
  "manifest_version": 3,
  "name": "Yahboom Tutorial Navigation",
  "version": "1.0",
  "description": "Navigate between Yahboom tutorials",
  "permissions": ["activeTab", "clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["https://www.yahboom.com/build.html*","https://yahboom-balancecar-doc.readthedocs.io/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_popup": "",
    "default_icon": "icon.png"
  }
}